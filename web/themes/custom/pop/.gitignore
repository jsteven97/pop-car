# Ignore configuration files that may contain sensitive information.
settings*.php

# Ignore paths that contain user-generated content.
sites/*/files
sites/*/private

# Ignore node_modules directory - this is used by many front-end tools.
node_modules/

# Ignore CSS and JS files if they are built from a source directory.
css/
js/

# Ignore SASS cache files
.sass-cache/

# Ignore vendor directory
vendor/

# Ignore IDE specific files
.idea/
*.iml
.vscode/
*.swp
*.swo
*.swn
*.DS_Store
